<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <PackageId>Siganberg.SerilogElasticSearch</PackageId>
        <Version>2.0.3</Version>
        <Authors>Francis Marasigan</Authors>
        <Company>Siganberg</Company>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageProjectUrl>https://github.com/francischie/Siganberg.SerilogElasticSearch</PackageProjectUrl>
        <PackageReleaseNotes>
            - Catch exception and log instead of swallowing.  
            - Fixed memory leak.
        </PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
        <FrameworkReference Include="Microsoft.AspNetCore.App" />
        <PackageReference Include="Serilog.AspNetCore" Version="5.0.0" />
        <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />

    </ItemGroup>



</Project>
