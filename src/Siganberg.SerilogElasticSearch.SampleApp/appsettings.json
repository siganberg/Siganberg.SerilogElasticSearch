{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "System.Net.Http.HttpClient.Default.LogicalHandler": "Information"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "RequestLoggingOptions" : {
      "IncludeResponseBody" : "true",
      "IncludeRequestHeaders" : "false",
      "ExcludeHeaderNames" : [
        "Authorization",
        "Host",
        "User-Agent"
      ]
    }
  }
}