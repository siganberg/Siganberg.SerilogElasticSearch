{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http.HttpClient.Default.LogicalHandler": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "RequestLoggingOptions" : {
      "IncludeResponseBody" : "true",
      "IncludeRequestHeaders" : "true",
      "ExcludeHeaderNames" : [
        "Authorization",
        "Origin",
        "Referer"
      ]
    }
  }
}